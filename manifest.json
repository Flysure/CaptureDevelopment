{
	"name": "Capture",
	"version": "1.0",
	"description": "Save your tabs away, along with notes, for future use.",
	"manifest_version": 2,
	"permissions": [
		"tabs",
		"storage"
	],
	"browser_action": {
		"default_popup": "popup/popup.html"
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Alt+S",
				"mac": "Alt+S"
			},
			"description": "Opens popup.html"
		}
	}
}